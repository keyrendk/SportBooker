<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profil">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/montserrat_extrabold"
        android:text="SportBooker"
        android:textColor="#F84995"
        android:textSize="130px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User Name"
        tools:layout_editor_absoluteX="170dp"
        tools:layout_editor_absoluteY="285dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="328dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gender"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="394dp" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="287dp"
        android:layout_height="47dp"
        android:ems="10"
        android:hint="male/female"
        android:inputType="text"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="411dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Order History"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="458dp" />

    <EditText
        android:id="@+id/editTextText4"
        android:layout_width="284dp"
        android:layout_height="49dp"
        android:ems="10"
        android:inputType="text"
        android:text="DANA :                           Rp 50.000"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="500dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress3"
        android:layout_width="288dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Whoho@unai.edu"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="350dp" />

    <EditText
        android:id="@+id/editTextText5"
        android:layout_width="284dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="text"
        android:text="BANK :                           Rp 50.000"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="548dp" />

    <Button
        android:id="@+id/button28"
        android:layout_width="282dp"
        android:layout_height="41dp"
        android:text="Log Out"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="636dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="148dp"
        tools:srcCompat="@tools:sample/avatars" />


</androidx.constraintlayout.widget.ConstraintLayout>